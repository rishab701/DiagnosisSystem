<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Background Bitmap -->
    <item>
        <bitmap
            android:src="@drawable/doctor" />
    </item>

    <!-- Stroke and Corners -->
    <item>
        <shape>
            <stroke
                android:width="2dp"
                android:color="@color/Lavender"/>
            <corners android:radius="20dp"/>
        </shape>
    </item>
</layer-list>

